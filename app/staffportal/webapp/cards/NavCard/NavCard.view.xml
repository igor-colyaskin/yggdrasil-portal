<mvc:View
    controllerName="com.epic.yggdrasil.staffportal.cards.NavCard.NavCard"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    xmlns="sap.m">
    <HBox alignItems="Center" justifyContent="SpaceBetween" class="sapUiSmallMarginEnd">
        <IconTabBar 
            id="navTabBar" 
            selectedKey="{ui>/currentTab}"
            select=".onTabSelect" 
            headerMode="Inline"
            class="sapUiNoContentPadding">
            <items>
                <IconTabFilter key="staff" text="Overview" icon="sap-icon://dimension" />
                <IconTabFilter key="finance" text="Finance" icon="sap-icon://money-bills" />
                <IconTabFilter key="registry" text="Registry" icon="sap-icon://project-definition-triangle" />
                <IconTabFilter key="magicProfile" text="Profile Feature" icon="sap-icon://customer-and-contacts" />
            </items>
        </IconTabBar>
    </HBox>
</mvc:View>