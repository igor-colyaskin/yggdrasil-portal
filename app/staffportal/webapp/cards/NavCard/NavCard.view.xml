<mvc:View
    controllerName="com.epic.yggdrasil.staffportal.cards.NavCard.NavCard"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m">
    <IconTabBar 
        id="navTabBar" 
        select=".onTabSelect" 
        headerMode="Inline" 
        stretchContentHeight="true"
        class="sapUiNoContentPadding">
        <items>
            <IconTabFilter key="staff" text="Overview" icon="sap-icon://dimension" />
            <IconTabFilter key="finance" text="Finance" icon="sap-icon://money-bills" />
            <IconTabFilter key="projects" text="Projects" icon="sap-icon://add-coursebook" />
        </items>
    </IconTabBar>
</mvc:View>