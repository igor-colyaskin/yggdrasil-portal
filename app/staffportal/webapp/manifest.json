{
  "_version": "1.60.0",
  "sap.app": {
    "id": "com.epic.nebula",
    "type": "application",
    "title": "Nebula Portal Engine",
    "applicationVersion": {
      "version": "1.0.0"
    }
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "com.epic.nebula.shell.Shell",
      "type": "XML",
      "async": true,
      "id": "shellView"
    },
    "dependencies": {
      "minUI5Version": "1.120.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.ui.integration": {}
      }
    },
    "resourceRoots": {
      "com.epic.nebula.lib": "./lib"
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.epic.nebula.i18n.i18n"
        }
      },
      "ui": {
        "type": "sap.ui.model.json.JSONModel"
      }
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "controlId": "nebulaApp",
        "controlAggregation": "pages",
        "async": true
      },
      "routes": [
        {
          "name": "home",
          "pattern": "",
          "target": "home"
        }
      ],
      "targets": {
        "home": {
          "viewName": "com.epic.nebula.shell.Shell"
        }
      }
    }
  }
}