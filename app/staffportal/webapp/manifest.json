{
  "_version": "1.60.0",
  "sap.app": {
    "id": "com.epic.yggdrasil.staffportal",
    "type": "application",
    "title": "Yggdrasil Saga Portal",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/hr/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0"
        }
      },
      "financeService": {
        "uri": "/odata/v2/finance",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "resourceRoots": {
      "com.sap.fiorireuselibrary.ui5cardssdk": "./lib/ui5cardssdk"
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "dependencies": {
      "minUI5Version": "1.120.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.epic.yggdrasil.staffportal.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server"
        }
      },
      "finance": {
        "dataSource": "financeService",
        "type": "sap.ui.model.odata.v2.ODataModel"
      },
      "ui": {
        "type": "sap.ui.model.json.JSONModel"
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "path": "com.epic.yggdrasil.staffportal.view",
        "controlId": "app",
        "controlAggregation": "pages",
        "async": true
      },
      "routes": [
        {
          "name": "RouteStuff",
          "pattern": "",
          "target": [
            "TargetStuff"
          ]
        }
      ],
      "targets": {
        "TargetStuff": {
          "id": "Stuff",
          "name": "Stuff",
          "viewType": "XML",
          "transition": "slide",
          "viewLevel": 1
        }
      }
    },
    "rootView": {
      "viewName": "com.epic.yggdrasil.staffportal.view.Stuff",
      "type": "XML",
      "id": "App"
    }
  }
}