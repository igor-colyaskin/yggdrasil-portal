{
  "_version": "1.76.0",
  "sap.app": {
    "id": "com.epic.yggdrasil.staffportal",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.20.0",
      "toolsId": "bc677586-25d7-4599-856d-e8191df904a7"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/hr/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      },
      "financeService": {
        "uri": "/odata/v2/finance/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "2.0"
        }
      },
      "projectService": {
        "uri": "/odata/v4/projects/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "resourceRoots": {
      "com.sap.fiorireuselibrary.ui5cardssdk": "./lib"
    },
    "dependencies": {
      "minUI5Version": "1.143.2",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "com.sap.fiorireuselibrary.ui5cardssdk": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.epic.yggdrasil.staffportal.i18n.i18n",
          "supportedLocales": [
            "",
            "en"
          ],
          "fallbackLocale": ""
        }
      },
      "": {
        "dataSource": "mainService",
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true
        }
      },
      "finance": {
        "dataSource": "financeService",
        "type": "sap.ui.model.odata.v2.ODataModel",
        "settings": {
          "defaultBindingMode": "TwoWay"
        }
      },
      "projects": {
        "dataSource": "projectService",
        "settings": {
          "synchronizationMode": "None"
        }
      },
      "ui": {
        "type": "sap.ui.model.json.JSONModel",
        "settings": {
          "defaultBindingMode": "TwoWay"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "com.epic.yggdrasil.staffportal.view",
        "async": true,
        "viewPath": "com.epic.yggdrasil.staffportal.view"
      },
      "routes": [
        {
          "name": "RouteStuff",
          "pattern": ":?query:",
          "target": [
            "TargetStuff"
          ]
        }
      ],
      "targets": {
        "TargetStuff": {
          "id": "Stuff",
          "name": "Stuff"
        }
      }
    },
    "rootView": {
      "viewName": "com.epic.yggdrasil.staffportal.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}