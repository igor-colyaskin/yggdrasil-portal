{
  "_version": "1.60.0",
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    },
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "tablet": ""
    }
  },
  "sap.app": {
    "id": "com.epic.yggdrasil.staffportal",
    "type": "application",
    "title": "Yggdrasil Saga Portal",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/hr/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0"
        }
      },
      "financeService": {
        "uri": "/odata/v2/finance/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0"
        }
      },
      "projectService": {
        "uri": "/odata/v4/projects/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui5": {
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "flexEnabled": true,
    "rootView": {
      "viewName": "com.epic.yggdrasil.staffportal.shell.Shell",
      "type": "XML",
      "async": true,
      "id": "shellView"
    },
    "dependencies": {
      "minUI5Version": "1.120.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.ui.integration": {}
      }
    },
    "resourceRoots": {
      "com.sap.fiorireuselibrary.ui5cardssdk": "./lib/sdkcard"
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.epic.yggdrasil.staffportal.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "type": "sap.ui.model.odata.v4.ODataModel",
        "settings": {
          "autoExpandSelect": true,
          "operationMode": "Server",
          "groupId": "$auto"
        }
      },
      "fin": {
        "dataSource": "financeService",
        "type": "sap.ui.model.odata.v2.ODataModel"
      },
      "proj": {
        "dataSource": "projectService",
        "type": "sap.ui.model.odata.v4.ODataModel"
      }
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "controlId": "staffApp",
        "controlAggregation": "pages",
        "async": true
      },
      "routes": [
        {
          "name": "master",
          "pattern": "",
          "target": "master"
        }
      ],
      "targets": {
        "master": {
          "viewName": "com.epic.yggdrasil.staffportal.shell.Shell"
        }
      }
    }
  }
}